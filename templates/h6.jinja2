      {# create new list item #}
      <li>{{key_6}}: 
        <input type="text" value='{{val_6}}'>
      </li>

    {% elif val_6 is mapping %}

      {# one-time entry #}
      {% set dict_7 = val_6 %}
      {% set header_7 = key_6 %}
      {% include 'h7.jinja2' %}

    {% else %}

      {% set list_6 = val_6 %}
      {% set test_item_6 = list_6[0] %}
      
      {% if ((test_item_6 is string) or (test_item_6 is number)) %}

        {# normal list #}
        <li>{{key_6}}
          <ul>
          {% for item_6 in list_6 %}
            <li>
              <input type="text", value='{{item_6}}'>
            </li> 
          {% endfor %}
          </ul>
        </li>

      {% else %}

        {# list of entries #}
        {% set header_7 = key_6 %}
        {% for dict_7 in list_6 %}
          {% include 'h7.jinja2' %}
        {% endfor %}

      {% endif %}

    {% endif %}

  {% endfor %}
  </ul>
